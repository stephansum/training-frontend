{
  "name": "training-frontend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "npm-run-all --parallel build:dev",
    "start": "npm run webserver",
    "webserver": "webpack-dev-server --open --env=dev --config webpack.config.dev.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean-dist",
    "prebuild:dev": "npm run prebuild",
    "prebuild:prod": "npm run prebuild",
    "build:dev": "webpack --env=dev --config webpack.config.dev.js",
    "build:prod": "webpack --env=prod --config webpack.config.prod.js",
    "localtunnel": "lt --port 3000",
    "predeploy:github": "npm run build:prod",
    "deploy:github": "gh-pages -d dist"
  },
  "dependencies": {
  },
  "devDependencies": {
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-file-include": "^0.14.0",
    "gulp-if": "^2.0.1",
    "gulp-jscs": "^4.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.11",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^5.0.0"
  }
}
